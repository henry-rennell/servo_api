            <p class="refresh-spotlight"> <a href="">refresh</a></p>
          </div>
          <div class="spotlight-station"></div>
        </section>
        <section class="stats">
            <h3>Stats</h3>
            <h4>total stations: <span class="total-stations"></span></h4>
            <h4>total owners: <span class="total-owners"></span></h4>
            <div class="owner-list"></div>
        </section>
        <section class="latest-prices">
            <h3>latest prices</h3>
            <p class="commodity-date"></p>
            <p>WTI oil <span class="wti"></span> USD per barrel üõ¢Ô∏è</p>
            <p>Brent oil <span class="brent"></span> USD per barrel üõ¢Ô∏è</p>
